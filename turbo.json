{
  "$schema": "https://turborepo.com/schema.json",
  "ui": "tui",
  "tasks": {
    "docker": {
      "outputs": []
    },
    "migrate": {
      "dependsOn": ["docker"],
      "outputs": []
    },
    "dev": {
      "dependsOn": ["migrate"],
      "cache": false,
      "persistent": true
    },
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*", "!tsconfig.tsbuildinfo"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**", "tsconfig.tsbuildinfo"]
    },
    "lint": {},
    "format": {},
    "check": {},
    "check:fix": {},
    "test": {
      "dependsOn": ["^build"]
    },
    "test:e2e": {},
    "test:coverage": {
      "outputs": ["coverage/**"]
    },
    "test:coverage:open": {
      "outputs": ["coverage/**"]
    },
    "storybook": {
      "cache": false,
      "persistent": true
    },
    "build-storybook": {
      "outputs": ["storybook-static/**"]
    }
  }
}
